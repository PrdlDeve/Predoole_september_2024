    <elementParameter field="MEMO_JAVA" name="CODE" value="context.card_id = input_row.card_id;&#xA;context.update_location = input_row.location;&#xA;context.update_lat = input_row.lat;&#xA;context.update_txn_time = input_row.txn_time;&#xA;context.update_lon =input_row.lon;&#xA;&#xA;&#xA;// Earth's radius in kilometers&#xA;final double R = 6371.0;&#xA; &#xA;// Default output values&#xA;output_row.card_id = input_row.card_id;&#xA;output_row.current_location = input_row.location;&#xA;output_row.previous_location = &quot;UNKNOWN&quot;;&#xA;output_row.distance_km = 0.0;&#xA;output_row.timeDiff = 0L;&#xA;output_row.is_geo_suspicious = false;&#xA; &#xA;// Always update last_location for future use&#xA;output_row.update_lat = input_row.lat != null ? input_row.lat.doubleValue() : 0.0;&#xA;output_row.update_lon = input_row.lon != null ? input_row.lon.doubleValue() : 0.0;&#xA;output_row.update_location = input_row.location;&#xA;output_row.update_txn_time = input_row.txn_time;&#xA; &#xA;// Null check (in case of missing last location info)&#xA;if (input_row.last_lat != null &amp;&amp; input_row.last_lon != null &amp;&amp; input_row.last_txn_time != null) {&#xA;    &#xA;    // Convert lat/lon to radians&#xA;    double lat1 = Math.toRadians(input_row.lat.doubleValue());&#xA;    double lon1 = Math.toRadians(input_row.lon.doubleValue());&#xA;    double lat2 = Math.toRadians(input_row.last_lat.doubleValue());&#xA;    double lon2 = Math.toRadians(input_row.last_lon.doubleValue());&#xA; &#xA;    // Haversine distance formula&#xA;    double dlat = lat1 - lat2;&#xA;    double dlon = lon1 - lon2;&#xA;    double a = Math.sin(dlat / 2) * Math.sin(dlat / 2)&#xA;             + Math.cos(lat1) * Math.cos(lat2) * Math.sin(dlon / 2) * Math.sin(dlon / 2);&#xA;    double c = 2 * Math.atan2(Math.sqrt(a), Math.sqrt(1 - a));&#xA;    double distance = R * c;&#xA; &#xA;    // Time difference in minutes&#xA;    long timeDiffMillis = input_row.txn_time.getTime() - input_row.last_txn_time.getTime();&#xA;    long timeDiffMinutes = timeDiffMillis / (1000L * 60L);&#xA; &#xA;    // Suspicious if distance > 500 KM and time &lt; 60 minutes&#xA;    boolean isSuspicious = (distance > 500 &amp;&amp; timeDiffMinutes >= 0 &amp;&amp; timeDiffMinutes &lt; 60);&#xA; &#xA;    // Assign computed values to output&#xA;    output_row.previous_location = input_row.last_location;&#xA;    output_row.distance_km = distance;&#xA;    output_row.timeDiff = timeDiffMinutes;&#xA;    output_row.is_geo_suspicious = isSuspicious;&#xA;}"/>
